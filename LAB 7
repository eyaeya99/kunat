a=10
b=6
if a > b:    
    print("a is greater than b")

x = 20
y = 50
if y > x:
    print(str(x) + " is greater than " + str(y))
else:
    print(str(x) + " is not greater than " + str(x))
una = 100
dos = 100
if una > dos:
    print(str(una) + " > " + str(dos))
    
elif dos == una:
    print(str(una) + " == " + str(dos))
else:
    print(str(una) + " < " + str(dos))
